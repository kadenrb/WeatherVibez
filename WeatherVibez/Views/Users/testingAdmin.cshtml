@{
    var claims = User.Claims.ToList();
}

<!-- This view was just used to test admin role, but its apart of my old testing so ill keep it here-->
<h2>User Claims</h2>
<ul>
    @foreach (var claim in claims)
    {
        <li>@claim.Type: @claim.Value</li>
    }
</ul>

